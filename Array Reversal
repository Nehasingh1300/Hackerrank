#include <stdio.h>
#include <stdlib.h>

int main() {
    int n;
    // i, arr;
    int *ptr;
    int sum=0;
    
    scanf("%d",&n);
    ptr = ( int* ) malloc (n * sizeof(int));
    for(int i=0;i<=n;i++){
        scanf("%d",ptr+i);
        
    }
    for(int i = n-1; i>-1; i--)
        printf("%d ", *(ptr + i));
        return 0 ;
}

